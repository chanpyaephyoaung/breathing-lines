      "dotenv": "^16.4.5",
      "eslint": "^8.55.0",
      "eslint-plugin-react": "^7.33.2",
      "eslint-plugin-react-hooks": "^4.6.0",
      "eslint-plugin-react-refresh": "^0.4.5",
      "jest": "^29.7.0",
      "mocha": "^10.3.0",
      "multer": "^1.4.5-lts.1",
      "nodemon": "^3.0.3",
      "supertest": "^6.3.4"
   },
